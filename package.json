{
  "name": "example api",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "format-file": "eslint --config eslint.config.mjs --no-config-lookup --fix",
    "format-check": "prettier --check -u \"./**/*.{ts,tsx,js,jsx,json}\"",
    "format-write": "prettier --write -u \"./**/*.{ts,tsx,js,jsx,json}\"",
    "generate:typescript": "./scripts/generate-typescript.sh",
    "generate:json2ts": "rimraf generated/typescript/json2ts/ && ts-node -r tsconfig-paths/register scripts/convertWithJson2Ts.ts --srcDir=types/json-schema --outDir=generated/typescript/json2ts/",
    "clean": "rimraf generated && rimraf dist",
    "generate": "npm run clean && npm run generate:typescript && REDOCLY_TELEMETRY=off redocly bundle openapi/islands-api.yaml -o generated/openapi/islands-api",
    "build": "npm run generate && npm run dist",
    "dist": "./scripts/dist.sh"
  },
  "author": "",
  "description": "",
  "dependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/compat": "^1.2.2",
    "@openapitools/openapi-generator-cli": "^2.21.3",
    "@redocly/cli": "^1.34.3",
    "@swc/core": "^1.11.29",
    "@swc/helpers": "^0.5.17",
    "@types/lodash": "^4.14.179",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-lodash": "^8.0.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^11.0.2",
    "json-schema-to-typescript": "^15.0.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "openapi-typescript": "^7.8.0",
    "quicktype": "^23.2.6",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "yamljs": "^0.3.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.33"
  }
}
